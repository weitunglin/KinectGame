<Window x:Class="KinectGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KinectGame"
        mc:Ignorable="d"
        Title="MainWindow" Height="900" Width="1560" ResizeMode="CanMinimize"
        Loaded="Kinect_Class2_Loaded" Unloaded="Kinect_Class2_Unloaded">
    <Grid>
        <TextBlock x:Name="txtLeft" Height="60" Margin="10,74,1410.6,736.4">txt left</TextBlock>
        <TextBlock x:Name="txtRight" Height="60" Margin="10,139,1410.6,671.4">txt right</TextBlock>
        <TextBlock x:Name="SpineShoulderDepthTxt" Height="60" Margin="10,210,1410.6,500">spineshoudlerDepth</TextBlock>
        <Image x:Name="ImageSource" HorizontalAlignment="Left" Height="720" Margin="148,10,0,0" VerticalAlignment="Top" Width="1280"/>

       
        <Canvas x:Name="ImageCanvas"  HorizontalAlignment="Left" Height="720" Margin="148,10,0,0" VerticalAlignment="Top" Width="1280" Background="Transparent"/>
        <Canvas>
            <Ellipse x:Name="Circle" Fill="#CCCCCC" Width="100" Height="100" Canvas.Left="100" Canvas.Top="200" ></Ellipse>
        </Canvas>
    </Grid>
</Window>
